<Page x:Class="RoRAssist.Pages.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RoRAssist.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="500"
      Title="MainPage">

    <Grid>
        <StackPanel HorizontalAlignment="Left" Height="136" Margin="71,84,0,0" VerticalAlignment="Top" Width="168">
            <Button x:Name="startButton"  Content="Start" Click="startButton_Click"/>
            <Button x:Name="quitButton" Content="Quit" Click="quitButton_Click"/>
        </StackPanel>

    </Grid>
</Page>
